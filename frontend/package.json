  "dependencies": {
    "next": "^15.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "axios": "^1.6.8",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.10",
    "zustand": "^5.0.6",
    "@tanstack/react-query": "^5.81.5",
    "jwt-decode": "^4.0.0",
    "nookies": "^2.5.2",
    "lucide-react": "^0.323.0",
    "next-seo": "^6.0.0",
    "react-hook-form": "^7.51.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.81.5",
    "typescript": "^5.8.3",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "@testing-library/react": "^15.0.0",
    "jest": "^30.0.0",
    "cypress": "^13.1.0",
    "eslint": "^8.57.0",                  
    "eslint-config-next": "^15.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.4",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
